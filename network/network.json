{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "variables": {
    "artifactsLocation": "https://raw.githubusercontent.com/rpbouw/azure-deployment/master/network/"
  },
  "resources": [
    {
      "apiVersion": "2015-01-01",
      "name": "vnet-westeurope-deployment",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(variables('artifactsLocation'), 'vnet-template', '.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parametersLink": { 
          "uri": "[concat(variables('artifactsLocation'), 'vnet-parameters-we', '.json')]",
          "contentVersion": "1.0.0.0"
        } 
      }
    },
    {
      "apiVersion": "2015-01-01",
      "name": "vnet-northeurope-deployment",
      "type": "Microsoft.Resources/deployments",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(variables('artifactsLocation'), 'vnet-template', '.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parametersLink": { 
          "uri": "[concat(variables('artifactsLocation'), 'vnet-parameters-ne', '.json')]",
          "contentVersion": "1.0.0.0"
        } 
      }
    }
  ]
}
